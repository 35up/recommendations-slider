{
  "name": "tslib-boilerplate",
  "version": "0.0.1",
  "description": "The base template for libraries made in typescript",
  "scripts": {
    "build": "rollup --config",
    "lint": "eslint --ext .js,.ts .",
    "test": "jest --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "prepublishOnly": "make ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/35up/tslib-boilerplate.git"
  },
  "keywords": [
    "bootstrap"
  ],
  "author": "35up GmbH <the.engineers@35up.com>",
  "license": "UNLICENCED",
  "bugs": {
    "url": "https://github.com/35up/tslib-boilerplate/issues"
  },
  "homepage": "https://github.com/35up/tslib-boilerplate#readme",
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@35up/eslint-config-35up-base": "^2.0.3",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/chai": "^4.2.22",
    "@types/chai-string": "^1.4.2",
    "@types/jest": "^27.0.2",
    "@types/sinon": "^10.0.4",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^27.2.4",
    "chai": "^4.3.4",
    "chai-string": "^1.5.0",
    "eslint": "^7.32.0",
    "jest": "^27.2.4",
    "jest-fetch-mock": "^3.0.3",
    "rollup": "^2.58.0",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "typescript": "^4.4.3"
  },
  "main": "build/tslib-boilerplate.esm.js",
  "types": "src"
}
